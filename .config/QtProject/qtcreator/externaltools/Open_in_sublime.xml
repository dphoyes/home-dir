<externaltool id="Open_in_sublime">
    <description>Open in Sublime</description>
    <displayname>SublimeText</displayname>
    <category>Text</category>
    <executable output="ignore" error="ignore" modifiesdocument="yes">
        <path>subl-wait</path>
        <arguments>%{CurrentDocument:FilePath}:%{CurrentDocument:Row}:%{CurrentDocument:Column}</arguments>
        <workingdirectory>%{CurrentDocument:Path}</workingdirectory>
    </executable>
</externaltool>
